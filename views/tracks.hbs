{{#each tracks}}

  <div class="track-container">
    <div class="track-text">
      <p class="track-title">{{this.name}}</p>
    </div>
    <audio class="track-audio" controls src={{this.preview_url}}>
      <a href={{this.preview_url}}>
        Download audio
      </a>
    </audio>
  </div>

{{/each}}